{
    "manifest_version": 2,
    "name": "Jupyter Notifier",
    "description": "",
    "version": "0.0.1",
    "permissions": [
        "storage",
        "notifications"
    ],
    "web_accessible_resources": [
        "assets/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "content.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "./popup/popup.html",
        "default_title": "jupyter notifier"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "icons": {
        "16": "./assets/icons/pngs/baseline_notifications_black_18dp.png",
        "48": "./assets/icons/pngs/baseline_notifications_black_48dp.png",
        "128": "./assets/icons/pngs/baseline_notifications_black_48dp.png"

    }
    
}